    "def reverse_string(my_string):\n",
    "    mystack = Stack()    \n",
    "    for char in my_string:\n",
    "        mystack.push(char)\n",
    "    string_out = \"\"\n",
    "    while mystack.is_empty() == False:\n",
    "        string_out += mystack.pop()\n",
    "    return string_out"
